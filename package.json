{
  "name": "jenkins-cypress-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "ci:build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cy:run-chrome": "cypress run --headless --browser chrome",
    "cy:run-firefox": "cypress run --headless --browser firefox -C cypress-firefox.json",
    "cy:verify": "cypress verify",
    "ci:start-ng-chrome": "angular-http-server --path ./dist/jenkins-cypress-test -p 4200",
    "ci:start-ng-firefox": "angular-http-server --path ./dist/jenkins-cypress-test -p 4201",
    "ci:cy-run-chrome": "start-server-and-test ci:start-ng-chrome http://localhost:4200 cy:run-chrome",
    "ci:cy-run-firefox": "start-server-and-test ci:start-ng-firefox http://localhost:4201 cy:run-firefox"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "angular-http-server": "^1.10.0",
    "cypress": "^8.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.3.2"
  }
}
